{
  "name": "aeditor",
  "version": "0.1.1",
  "description": "基于 Vue 可以输入公式的富文本编辑器",
  "main": "lib/aeditor.umd.min.js",
  "keyword": "editor aeditor 富文本",
  "private": false,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --target lib --name aeditor --dest lib packages/index.js"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "core-js": "^3.3.2",
    "html2canvas": "^1.0.0-rc.5",
    "vue": "^2.6.10",
    "vue-color": "^2.7.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
